                <term_definitions code="at0000">
                  <items id="description">demografische Daten die Geburt betreffend</items>
                  <items id="text">Daten zur Geburt</items>
                </term_definitions>
                <term_definitions code="at0001">
                  <items id="description">Das Geburtsdatum der Person.</items>
                  <items id="text">Geburtsdatum</items>
                </term_definitions>
                <term_definitions code="at0002">
                  <items id="description">Das Geburtsland der Person.</items>
                  <items id="text">Geburtsland</items>
                </term_definitions>
                <term_definitions code="at0003">
                  <items id="description">Ein Indikator für Mehrlingsgeburten. Er gibt die Anzahl der geborenen Personen an, die aus einer Schwangerschaft resultierten.</items>
                  <items id="text">Anzahl der geborenen Personen</items>
                </term_definitions>
                <term_definitions code="at0004">
                  <items id="description">Die Nummer der Kinder bei einer Mehrlingsgeburt, unabhängig davon ob es sich um Lebend- oder Totgeburten handelte.</items>
                  <items id="text">Geburtsreihenfolge</items>
                </term_definitions>
                <term_definitions code="at0005">
                  <items id="description">Kennzeichen das angibt, ob es
notwendig ist das Geburtsdatum
nachzuverfolgen, um ein genaueres
Datum zu erhalten.</items>
                  <items id="text">Ein Indikator für die Nachverfolgung eines Geburtsdatums.</items>
                </term_definitions>
                <term_definitions code="at0006">
                  <items id="description">Zusätzliche länderspezifische Daten die Geburt betreffend</items>
                  <items id="text">Länderspezifische Daten</items>
                </term_definitions>
